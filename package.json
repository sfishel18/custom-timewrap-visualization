{
  "name": "viz_timewrap",
  "version": "1.0.0",
  "description": "This app adds a custom time-wrap visualization to your Splunk instance. It can be used to compare a single metric over multiple time periods.",
  "scripts": {
    "package": "gulp",
    "build": "NODE_ENV=production webpack",
    "devbuild": "webpack --progress",
    "watch": "webpack -d --watch --progress",
    "lint": "eslint .",
    "karma": "karma start",
    "prevrt:docker": "docker pull selenium/standalone-chrome:2.53.0",
    "vrt:docker": "docker run -d --name selenium -p 4444:4444 -v $(pwd)/vrt:/vrt selenium/standalone-chrome",
    "vrt:build": "webpack --config vrt/webpack.config.js",
    "prevrt": "npm run postvrt && npm run vrt:docker && npm run vrt:build",
    "vrt": "mocha --timeout 30000 -u tdd vrt/tests/**/*-test.js",
    "postvrt": "docker stop selenium && docker rm selenium || true",
    "test": "npm run lint -s && npm run karma -s -- --single-run && npm run vrt -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfishel18/custom-timewrap-visualization.git"
  },
  "author": "Simon Fishel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sfishel18/custom-timewrap-visualization/issues"
  },
  "homepage": "https://github.com/sfishel18/custom-timewrap-visualization#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-mocha": "^4.5.1",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "git-rev-sync": "^1.7.1",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace-task": "^0.11.0",
    "gulp-tar": "^1.9.0",
    "karma": "^1.2.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "node-resemble-js": "^0.1.1",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-selector-namespace": "^1.2.4",
    "react-addons-test-utils": "^15.3.1",
    "rimraf": "^2.5.4",
    "selenium-webdriver": "^3.0.0-beta-2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "lodash": "^4.16.1",
    "moment": "^2.15.1",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-addons-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-vis": "^0.6.3"
  }
}

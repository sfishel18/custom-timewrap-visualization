{
  "name": "viz_timewrap",
  "version": "2.0.0",
  "description": "This app adds a custom time-wrap visualization to your Splunk instance. It can be used to compare a single metric over multiple time periods.",
  "scripts": {
    "package": "gulp",
    "build": "NODE_ENV=production webpack",
    "devbuild": "webpack --progress",
    "watch": "webpack -d --watch --progress",
    "lint": "eslint .",
    "karma": "karma start",
    "prevrt:docker": "docker pull selenium/standalone-chrome:3.2.0",
    "vrt:docker": "docker run -d --name selenium -p 4444:4444 -v $(pwd)/vrt:/vrt selenium/standalone-chrome:3.2.0",
    "vrt:build": "webpack --config vrt/webpack.config.js",
    "prevrt": "yarn run postvrt && yarn run vrt:docker && yarn run vrt:build",
    "vrt": "mocha --timeout 30000 -u tdd vrt/tests/**/*-test.js",
    "postvrt": "docker stop selenium && docker rm selenium || true",
    "test": "yarn run lint && yarn run karma -- --single-run && yarn run vrt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfishel18/custom-timewrap-visualization.git"
  },
  "author": "Simon Fishel",
  "license": "MIT",
  "readme": "README.md",
  "bugs": {
    "url": "https://github.com/sfishel18/custom-timewrap-visualization/issues"
  },
  "homepage": "https://github.com/sfishel18/custom-timewrap-visualization#readme",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "^3.5.0",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.5.1",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "git-rev-sync": "^1.9.0",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace-task": "^0.11.0",
    "gulp-tar": "^1.9.0",
    "karma": "1.6.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "2.0.3",
    "mocha": "^3.0.2",
    "node-resemble-js": "^0.1.1",
    "postcss-import": "9.1.0",
    "postcss-loader": "^1.0.0",
    "postcss-selector-namespace": "1.4.1",
    "rimraf": "^2.5.4",
    "selenium-webdriver": "^3.0.0-beta-2",
    "style-loader": "0.16.1",
    "webpack": "2.4.1"
  },
  "dependencies": {
    "jquery": "3.2.1",
    "jquery-powertip": "^1.3.0",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "plottable": "3.0.0"
  }
}
